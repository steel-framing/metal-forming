@using MetalForming.Common
<!DOCTYPE html>
<html lang="es-ES">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0" />
    <title>METAL FORMING</title>

    <meta name="description" content="METAL FORMING" />
    <link rel="shortcut icon" type="image/x-icon" href="@Url.Content("~/favicon.ico")" />

    <link rel="stylesheet" href="@Url.Content("~/Content/css/theme.min.css")" />
    <link rel=" stylesheet" href="@Url.Content("~/Content/css/datepicker.min.css")" />
    <!-- plugins -->
    <link href="@Url.Content("~/Content/ionicons/css/ionicons.min.css")" rel="stylesheet" />
    <!-- styles -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/style.css")" type="text/css" />
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    @RenderSection("styles", required: false)
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="javascript:;">
                    <h3>METAL-FORMING &nbsp;&nbsp;</h3>
                </a>
            </div>
            <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="@(ViewBag.MenuActual == ConstantesWeb.Menu.Plan ? "active" : "")"><a href="javascript:void(0);">@ConstantesWeb.Menu.Plan</a></li>
                    <li class="dropdown @(ViewBag.MenuActual == ConstantesWeb.Menu.Programacion ? "active" : "")">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="javascript:void(0);">
                            @ConstantesWeb.Menu.Programacion
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li class="@(ViewBag.MenuItemActual == ConstantesWeb.MenuItem.MantenerPrograma ? "active" : "")"><a href="@Url.Action("MantenerPrograma", "Programacion")">@ConstantesWeb.MenuItem.MantenerPrograma</a></li>
                            <li class="@(ViewBag.MenuItemActual == ConstantesWeb.MenuItem.MantenerOrdenProduccion ? "active" : "")"><a href="@Url.Action("MantenerOrdenProduccion", "Programacion")">@ConstantesWeb.MenuItem.MantenerOrdenProduccion</a></li>
                        </ul>
                    </li>
                    <li class="dropdown @(ViewBag.MenuActual == ConstantesWeb.Menu.Produccion ? "active" : "")">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="javascript:void(0);">
                            @ConstantesWeb.Menu.Produccion
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li class="@(ViewBag.MenuItemActual == ConstantesWeb.MenuItem.EjecutarOrdenProduccion ? "active" : "")"><a href="@Url.Action("Index", "Produccion")">@ConstantesWeb.MenuItem.EjecutarOrdenProduccion</a></li>
                            <li class="@(ViewBag.MenuItemActual == ConstantesWeb.MenuItem.AsignarOrdenProduccion ? "active" : "")"><a href="javascript:void(0);">@ConstantesWeb.MenuItem.AsignarOrdenProduccion</a></li>
                        </ul>
                    </li>
                    <li class="@(ViewBag.MenuActual == ConstantesWeb.Menu.Seguimiento ? "active" : "")"><a href="javascript:void(0);">@ConstantesWeb.Menu.Seguimiento</a></li>
                    <li class="@(ViewBag.MenuActual == ConstantesWeb.Menu.Seguridad ? "active" : "")"><a href="javascript:void(0);">@ConstantesWeb.Menu.Seguridad</a></li>
                    <li class="dropdown @(ViewBag.MenuActual == ConstantesWeb.Menu.Mantenimiento ? "active" : "")">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="javascript:void(0);">
                            @ConstantesWeb.Menu.Mantenimiento
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li class="@(ViewBag.MenuItemActual == ConstantesWeb.MenuItem.MantenerMaquina ? "active" : "")"><a href="javascript:void(0);">@ConstantesWeb.MenuItem.MantenerMaquina</a></li>
                            <li class="@(ViewBag.MenuItemActual == ConstantesWeb.MenuItem.MantenerMaterial ? "active" : "")"><a href="javascript:void(0);">@ConstantesWeb.MenuItem.MantenerMaterial</a></li>
                            <li class="@(ViewBag.MenuItemActual == ConstantesWeb.MenuItem.MantenerProducto ? "active" : "")"><a href="javascript:void(0);">@ConstantesWeb.MenuItem.MantenerProducto</a></li>
                            <li class="@(ViewBag.MenuItemActual == ConstantesWeb.MenuItem.MantenerPLC ? "active" : "")"><a href="javascript:void(0);">@ConstantesWeb.MenuItem.MantenerPLC</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    @RenderSection("breadcrumb", required: false)

    @RenderBody()

    <footer class="footer-page">
        <div class="container">
            &copy; Metal Forming 2015. Todos los derechos reservados.
            <div class="pull-right">
                o&amp;c
            </div>
        </div>
    </footer>

    <div class="modalAjax"></div>

    <!-- core -->
    <script src="@Url.Content("~/Content/js/jquery-1.11.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/bootstrap-datepicker.min.js")" ty type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/bootstrap-datepicker.es.js")" ty type="text/javascript"></script>
    <!-- plugins -->
    <!-- script -->
    <script src="@Url.Content("~/Content/js/app.js")" type="text/javascript"></script>
    <script type="text/javascript">
        $(function () {
            app.init();
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
